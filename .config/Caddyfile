localhost:8080 {
	proxy / vue:8080 {
		transparent
	}
	proxy /api web:8080 {
		transparent
		without /api
	}
}

localhost:8081 {
	proxy / mongo-express:8081
}

http://noj.tw {
 	tls admin@noj.tw
	proxy / vue:8080 {
		transparent
	}
    proxy /api web:8080 {
 		transparent
 		without /api
 	}
}

http://www.noj.tw {
	redir http://noj.tw{uri}
}

http://mongo.noj.tw {
 	tls admin@noj.tw
	proxy / mongo-express:8081
}